    function addNewTemplate() {
        var idx = $('#template_num').text();
        $("#template_group").append('<div id="template_' + idx + '"></div>')
        $("#template_" + idx).append($("#origin_template").html());
        changeIdAndName($("#template_" + idx), idx);
        // 事件绑定
        var cls = $('#class').val();
        for (var i = 0; i < adsType[cls].bindAction.length; i++) {
            adsType[cls].bindAction[i](idx);
        }
        // 绑定验证
        for (var i = 0; i < adsType[cls].validation.length; i++) {
            (function () {
                var cfg = checkObj[adsType[cls].validation[i]];
                if (cfg.isBlur) {
                    $('#' + adsType[cls].validation[i] + '_' + idx).blur(function () {
                        if (!cfg.validationFunc($(this).val(), $(this))) {
                            (cfg.errorFunc || vaError)($(this), cfg.errorInfo);
                        } else {
                            (cfg.successfunc || vaSuccess)($(this));
                        }
                    })
                }
            })();
        }

        $('select[name=splash_time_' + idx + ']').selectpicker();
        $('#base_template_' + idx).html($('#' + $('#class').val()).html());
        $("#expand_template_" + idx).click();
        $("#template_num").text(parseInt(idx) + 1);
        templateNumSuccess($('#template_group'));
        return idx;
    }

