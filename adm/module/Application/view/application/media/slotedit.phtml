    function setTemplateData() {
        var cfg = adsType[$('#class').val()];
        var dataObj = {};
        for (var i in objValMapping) {
            var type = objValMapping[i].type;
            var objList = $(objValMapping[i].label + '[name^=' + objValMapping[i].name + '_]');
            for (var j = 0; j < objList.length; j++) {
                var idx = getIdxByName($(objList[j]).attr('name'));
                dataObj[idx] = dataObj[idx] || {};
                if (objType[type].getVal($(objList[j]))) {
                    dataObj[idx][objValMapping[i].name] = objType[type].getVal($(objList[j]));
                }
            }
        }
        return dataObj;
    }

    // 目前只写广告位名称，如果改ajax提交记得补全！！！
    function setSlotData() {
        var dataObj = {};
        dataObj.uid = $('#uid_hide').val();
        dataObj.name = $('#name').val();
        return dataObj;
    }

    function sloteditValidate() {
        $("#Deepleaper_loading").show();
        var valid = true;
        if (!vaIsNotNull($('#name').val()) || !vaWordNumberLimit1_25($('#name').val())) {
            vaError($('#name'), '名称长度应为1-50个字符:汉字占2个字符');
            $('#name').focus();
            valid = false;
        } else {
            vaSuccess($('#name'));
        }


        var vaList = adsType[$('#class').val()].validation;
        for (var k = 0; k < vaList.length; k++) {
            var cfg = checkObj[vaList[k]];
            if (cfg.batchCheck) {
                var objList = $(cfg.batchCheck);
                for (var i = 0; i < objList.length; i++) {
                    if (!$(objList[i]).parents('#origin_template').length) {
                        if (!cfg.validationFunc($(objList[i]).val(), $(objList[i]))) {
                            (cfg.errorFunc || vaError)($(objList[i]), cfg.errorInfo);
                            $("#Deepleaper_loading").hide();
                            return false;
                        } else {
                            (cfg.successFunc || vaSuccess)($(objList[i]));
                        }
                    }
                }
            }
        }

        // 后台验证样式名是否重复

        $.ajax({
            url: '/media/isSlotValid',
            data: {
                slot: setSlotData(),
                template: setTemplateData()
            },
            dataType: 'json',
            async: false,
            type: 'POST',
            success: function (ret) {
                console.log(ret);
                if (ret.status && ret.data) {
                    if (!ret.data.slot.name.res) {
                        vaError($('#name'), ret.data.slot.name.error);
                        valid = false;
                    } else {
                        vaSuccess($('#name'));
                    }
                    for (var idx in ret.data.template) {
                        if (!ret.data.template[idx].res) {
                            vaError($('#template_name_' + idx), ret.data.template[idx].error);
                            $('#template_body_' + idx).show();
                            valid = false;
                        } else {
                            vaSuccess($('#template_name_' + idx));
                            $('#template_body_' + idx).hide();
                        }
                    }
                }
            },
        })

        $('input').removeProp('disabled');
        $('select').removeProp('disabled');
        $('textarea').removeProp('disabled');
        $("#Deepleaper_loading").hide();
        return valid;
    }

