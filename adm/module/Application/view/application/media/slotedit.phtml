    function fetchTraditionalSlotAPIonSuccess(result) {
        var req = JSON.parse(result);
        if (req.status == '1') {
            var data = req.data;
            var cfg = adsType[data.class];
            if (data.templates) {
                for (var i = 0; i < data.templates.length; i++) {
                    // 填数据
                    var idx = addNewTemplate();
                    for (var j in objValMapping) {
                        var val = data.templates[i][objValMapping[j].fetchKey];
                        if (val) {
                            objType[objValMapping[j].type].setVal(objValMapping[j].name + '_' + idx, val);
                        }
                    }

                    // disabled相应控件
                    for (var j = 0; j < cfg.disabled.length; j++) {
                        var objCfg = objValMapping[cfg.disabled[j]];
                        $(objCfg.label + '[name=' + cfg.disabled[j] + '_' + idx + ']').prop('disabled', 'disabled');
                    }

                    $('#template_name_' + idx).blur();
                    $('#delete_template_' + idx).hide();
                    $('#soft_delete_template_' + idx).show();
                    $('#soft_delete_template_' + idx).bind('click', function (uid) {//删除广告位样式
                        return function () {
                            layer.confirm('确定删除该样式吗？', {
                                btn: ['确定', '取消'] //按钮
                            }, function (index, layero) {
                                deleteslottemplateAPI(uid, function (result) {
                                    var req = JSON.parse(result);
                                    if (req.status == 1) {
                                        $("#template_" + idx).remove();
                                    } else {
                                        layer.msg(req.msg)
                                    }
                                });
                                layer.close(index);
                            }, function () {
                            });
                        };
                    }(data.templates[i].st_uid));
                    $('input[name=slot_screen_' + idx + ']').change();
                }

                if (data.templates.length) {
                    $('#expand_template_' + idx).click();
                }
            }
        } else {
            layer.msg("获取广告位样式信息失败：" + req.msg);
        }
    }

