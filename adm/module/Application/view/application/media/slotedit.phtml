    // 随手递归一发修改所有id和name，在其值后面加上idx做为唯一标识
    function changeIdAndName(p, idx) {
        var len = $(p).children().length;
        for (var i = 0; i < len; i++) {
            node = $(p).children()[i];
            if ($(node).attr('id') !== undefined) {
                $(node).attr('id', $(node).attr('id') + '_' + idx);
            }
            if ($(node).attr('name') !== undefined) {
                $(node).attr('name', $(node).attr('name') + '_' + idx);
            }
            changeIdAndName($(node), idx);
        }

    }
