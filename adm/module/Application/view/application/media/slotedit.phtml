    // 逻辑是，如果点了某个样式展开，其他样式全部折叠，如果点了展开的样式折叠，则所有样式均折叠
    function bindExpandTemplateForm(idx) {
        $("#expand_template_" + idx).click(function () {
            var templateNum = $("#template_num").text();
            var targetIdx = $(this).attr('id').split('_');
            targetIdx = targetIdx[targetIdx.length - 1];
            if ($("#expand_template_" + targetIdx).text() == '折叠') {
                $("#template_body_" + targetIdx).hide();
                $("#expand_template_" + targetIdx).text('展开');
            } else {
                for (var i = 0; i < templateNum; i++) {
                    $("#template_body_" + i).hide();
                    $("#expand_template_" + i).text('展开');
                }
                $("#template_body_" + targetIdx).show();
                $("#expand_template_" + targetIdx).text('折叠');
            }
        });
    }

    // 样式名字输入框和样式标题联动，这样在用户折叠样式的时候他才能识别出不同样式
