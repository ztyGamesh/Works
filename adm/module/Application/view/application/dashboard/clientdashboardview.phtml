        for (var j = 0; j < show.children.length; j++) {
            var info = show.children[j].getAttribute('info');
            if (info === 'time') {
                if(time.length <= 10 ){
                    // 不折行
                    // 字体大
                    show.children[j].innerHTML = time;
                    $($("#topNow").children()[0]).css("fontSize","20px");
                    $($("#topBefore").children()[0]).css("fontSize","12px");
