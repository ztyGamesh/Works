  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "test:watch": "NODE_ENV=development ./node_modules/.bin/mocha -w test/demo.test.js --compilers js:babel-core/register,css:test/css-ignore-compiler.js",
    "test:cover": "NODE_ENV=production babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-core/register,css:test/css-ignore-compiler.js --recursive",
    "build-dev:webpack": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.dev.config.js --progress",
    "build-dev": "npm run clean && npm run build-dev:webpack",
    "build-test:webpack": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.test.config.js --progress",
    "build-test": "npm run clean && npm run build-test:webpack",
    "build-prod:webpack": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.prod.config.js --progress",
    "build-prod": "npm run clean && npm run build-prod:webpack",
    "build-local:webpack": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.local.config.js --progress",
    "build-local": "npm run clean && npm run build-local:webpack",
    "watch": "./node_modules/.bin/webpack-dev-server --progress --open"
  },
