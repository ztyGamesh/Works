<script type="text/javascript">

    // ------提交验证
    function slotError(obj, msg) {
        var errObj = $(obj).parent().parent().find('.help-block');
        errObj.html(msg).show();
        errObj.parents('.form-group').addClass('has-error');
        var nameList = $(obj).attr('id').split('_');
        var idx = nameList[nameList.length - 1];
        if (nameList.length > 1 && isNumber(idx) && $('#expand_template_' + idx).html() == '展开') {
            $('#expand_template_' + nameList[nameList.length - 1]).click();
        }
    }

    function slotSuccess(obj) {
        var errObj = $(obj).parent().parent().find('.help-block');
        errObj.html('').hide();
        errObj.parents('.form-group').removeClass('has-error');
    }

    function templateNumError(obj, msg) {
        var errObj = $(obj).find('.help-block');
        errObj.html(msg).show();
        errObj.parents('.form-group').addClass('has-error');
    }

    function templateNumSuccess(obj) {
        var errObj = $(obj).find('.help-block');
        errObj.html('').hide();
        errObj.parents('.form-group').removeClass('has-error');
    }

    // 如果折叠了要展开显示
    function slotAddValidError(obj, msg) {
        vaError(obj, msg);
        var nameList = $(obj).attr('id').split('_');
        var idx = nameList[nameList.length - 1];
        if (nameList.length > 1 && isNumber(idx) && $('#expand_template_' + idx).html() == '展开') {
            $('#expand_template_' + nameList[nameList.length - 1]).click();
        }
    }

    function hasTemplate(obj) {
        return $('#template_group').children().length > 1;
    }

    function isSlotNumber(val) {
        return $('#class').val() == '3e2bbd3f-3e8a-4ddc-892a-12070890543c' || isNumber(val);
    }

    function isFileUpload() {
        if ($('#class').val() != '3e2bbd3f-3e8a-4ddc-892a-12070890543c') {
            return true;
        }
        var objList = $('input[name^=file_s_1242_375_]');
        for (var i = 0; i < objList.length; i++) {
            var idx = $(objList[i]).attr('name').split('_');
            idx = idx[idx.length - 1];
            if ($('input[name=slot_screen_' + idx + '][value=half]').prop('checked')) {
                if (!$(objList[i]).val()) {
                    slotAddValidError($(objList[i]), '请上传企业logo图片')
                    return false;
                } else {
                    vaSuccess($(objList[i]));
                }
            }
        }
        return true;
    }

    var checkObj = {
        '#template_group': [hasTemplate, '请添加广告位样式', templateNumError],
        '#name': [vaIsNotNull, '广告位名称不能为空', vaError],
        '#name': [vaWordNumberLimit1_25, '名称长度应为1-50个字符:汉字占2个字符', vaError],
        '.template_name': [vaIsNotNull, '样式名称不能为空', slotAddValidError],
        '.slot_h': [isSlotNumber, '广告位尺寸格式错误', slotError],
        '.slot_w': [isSlotNumber, '广告位尺寸格式错误', slotError],
        '.price': [vaIsMoney, '刊例价格式错误', slotAddValidError]
    };

    // 目前只验证各种名称
    function putSlotData() {
        return {name: $('#name').val()};
    }

    function putTemplateData() {
        var ret = {};
        var templateNameList = $('input[name^=template_name_]');
        for (var i = 0; i < templateNameList.length; i++) {
            var name = $(templateNameList[i]).attr('name');
            var idx = name.split('_')[name.split('_').length - 1];
            ret[idx] = {};
            ret[idx]['template_name'] = $('#template_name_' + idx).val();
        }
        return ret;
    }

    function slotaddValidate() {
        $("#Deepleaper_loading").show();
        for (var objStr in checkObj) {
            var objList = $(objStr);
            for (var i = 0; i < objList.length; i++) {
                var obj = objList[i];
                if (!checkObj[objStr][0]($(obj).val()) && $(obj).parents('#origin_template').length == 0) {
                    checkObj[objStr][2]($(obj), checkObj[objStr][1]);
                    $("#Deepleaper_loading").hide();
                    return false;
                }
            }
        }
        if (!isFileUpload()) {
            $("#Deepleaper_loading").hide();
            return false;
        }

        var valid = true;
        $.ajax({
            url: '/media/isSlotValid',
            data: {
                slot: putSlotData(),
                template: putTemplateData()
            },
            dataType: 'json',
            async: false,
            type: 'POST',
            success: function (ret) {
                console.log(ret);
                if (ret.status && ret.data) {
                    if (!ret.data.slot.name.res) {
                        vaError($('#name'), ret.data.slot.name.error);
                        valid = false;
                    } else {
                        vaSuccess($('#name'));
                    }
                    for (var idx in ret.data.template) {
                        if (!ret.data.template[idx].res) {
                            vaError($('#template_name_' + idx), ret.data.template[idx].error);
                            $('#template_body_' + idx).show();
                            valid = false;
                        } else {
                            vaSuccess($('#template_name_' + idx));
                            $('#template_body_' + idx).hide();
                        }
                    }
                }
            }

        });

        $("#Deepleaper_loading").hide();
        return valid;
    }


</script>


