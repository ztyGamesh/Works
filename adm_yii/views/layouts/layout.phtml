        function showValidMenu(data) {
            var hasChild = {};
            for (var i = 0; i < data.length; i++) {
                var rId = data[i].url.replace(/\//g, '\\/');
                if (data[i].authority == 0 && data[i].pid != '/') {//非一级目录，无权限，隐藏此二级目录
                    $('#' + rId).hide();
                } else {
                    hasChild[data[i].pid] = true;//有权限的一级目录
                }
            }
            for (var i = 0; i < data.length; i++) {

                if (hasChild[data[i].url] && data[i].pid == '/') {//有权限，一级目录，显示此一级目录
                    var rId = data[i].url.replace(/\//g, '\\/');
                    $('#' + rId).show();
                }
